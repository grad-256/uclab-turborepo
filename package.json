{
  "name": "uclab-turborepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test",
    "sb": "turbo run storybook",
    "format": "prettier -w \"**/*.{ts,tsx}\""
  },
  "devDependencies": {
    "prettier": "^2.8.7",
    "turbo": "^1.9.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
